echo "Test X: (shortest distance, number of paths)"

echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1

# Additional test cases
echo "ADDITIONAL TEST CASES:"
echo "TEST 4: (Move only North)"
./greedy_robot 3 1 1 1 4
echo "TEST 5: (Move only South)"
./greedy_robot 3 1 4 1 1
echo "TEST 6: (Move only East)"
./greedy_robot 3 1 1 4 1
echo "TEST 7: (Move only West)"
./greedy_robot 3 4 1 1 1
echo "TEST 8: (Move in a straight line)"
./greedy_robot 5 1 1 5 1
echo "TEST 9: (Move in a square)"
./greedy_robot 5 1 1 5 5
echo "TEST 10: (Move diagonally)"
./greedy_robot 5 1 1 5 5
echo "TEST 11: (Move in a zigzag pattern)"
./greedy_robot 5 1 1 5 5
echo "TEST 12: (Move to negative coordinates)"
./greedy_robot 5 -3 -3 -1 -1

echo "Testing complete."
